@model dotNetFrameworkMVC_MoshProject.Models.Customer
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.Membership.Name</li>
    @if (Model.BirthDate != null)
    {
        <li>Birth Date: @Model.BirthDate.Value.ToShortDateString()</li>
    }
</ul>

